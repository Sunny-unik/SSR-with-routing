{
  "version": 2,
  "builds": [
    { "src": "*.ico", "use": "@vercel/static" },
    { "src": "public/(.*)", "use": "@vercel/static" },
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/favicon.ico",
      "dest": "public/favicon.ico"
    },
    {
      "src": "/app.js",
      "dest": "public/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
